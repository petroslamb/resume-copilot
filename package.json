{
  "name": "starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:agent": "mastra dev",
    "dev:ui": "next dev --turbopack",
    "dev": "concurrently \"pnpm run dev:agent\" \"pnpm run dev:ui\"",
    "dev:debug": "LOG_LEVEL=debug pnpm run dev",
    "build:ui": "next build",
    "start:ui": "next start",
    "build:agent": "mastra build",
    "start:agent": "mastra start",
    "lint": "next lint",
    "build": "mastra build && next build",
    "start": "concurrently \"pnpm run start:agent\" \"pnpm run start:ui\""
  },
  "dependencies": {
    "@ag-ui/mastra": "0.1.0",
    "@copilotkit/react-core": "1.10.6",
    "@copilotkit/react-ui": "1.10.6",
    "@copilotkit/runtime": "1.10.6",
    "@libsql/client": "^0.15.15",
    "@mastra/core": "0.19.1",
    "@mastra/libsql": "0.15.0",
    "@mastra/mcp": "^0.13.4",
    "@mastra/memory": "0.15.4",
    "dotenv": "^17.2.3",
    "libsql": "^0.5.22",
    "mastra": "0.13.4",
    "next": ">=15.5.4",
    "ollama-ai-provider-v2": "^1.5.0",
    "pdf-parse": "^2.4.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
